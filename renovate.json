{
    "extends": [
        "github>harvester/renovate:renovate-default"
    ],
    "baseBranches": [
        "master",
        "v1.5",
        "v1.6"
    ],
    "packageRules": [
        {
            "description": "Block specific Go modules from major/minor/patch version updates",
            "matchManagers": [
                "gomod"
            ],
            "matchPackageNames": [
                "github.com/k8snetworkplumbingwg/whereabouts"
            ],
            "matchUpdateTypes": [
                "major",
                "minor",
                "patch"
            ],
            "enabled": false
        },
        {
            "description": "Block specific image updates for major/minor/patch version updates",
            "matchManagers": [
                "docker"
            ],
            "enabled": false
        }
    ]
}
